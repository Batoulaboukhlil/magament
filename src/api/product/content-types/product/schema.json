{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "description": {
      "type": "text",
      "required": true,
      "minLength": 5
    },
    "quantity": {
      "type": "biginteger",
      "min": "0",
      "required": true
    },
    "lowStockThreshold": {
      "type": "biginteger"
    },
    "notification": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "related_product"
    },
    "inventory_log": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::nventory-log.nventory-log",
      "inversedBy": "product"
    },
    "address": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::address.address"
    },
    "supplier": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::supplier.supplier"
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "product"
    }
  }
}
