{
  "kind": "collectionType",
  "collectionName": "suppliers",
  "info": {
    "singularName": "supplier",
    "pluralName": "suppliers",
    "displayName": "Supplier"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "supplierID": {
      "type": "uid"
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "required": true
    },
    "contactInfo": {
      "type": "json"
    },
    "statusOf": {
      "type": "boolean",
      "required": false
    },
    "createdAtDate": {
      "type": "date"
    },
    "updatedAtDate": {
      "type": "date"
    },
    "deliveries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::delivery.delivery",
      "mappedBy": "supplier"
    }
  }
}
